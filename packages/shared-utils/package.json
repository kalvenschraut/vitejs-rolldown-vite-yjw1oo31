{
  "name": "@local/shared-utils",
  "version": "1.0.0",
  "type": "module",
  "main": "./dist/shared-utils.js",
  "types": "./dist/shared-utils.d.ts",
  "exports": {
    ".": {
      "types": "./dist/shared-utils.d.ts",
      "import": "./dist/shared-utils.js"
    },
    "./worker": {
      "types": "./dist/worker.d.ts",
      "import": "./dist/worker.js"
    }
  },
  "imports": {
    "#src/*": "./src/*",
    "#test/*": "./test/*"
  },
  "files": [
    "dist"
