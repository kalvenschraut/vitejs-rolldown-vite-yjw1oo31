  "scripts": {
    "build": "vite build && vue-tsc --declaration --emitDeclarationOnly --noCheck"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^6.0.0",
    "quasar": "^2.18.1",
    "sass-embedded": "^1.89.2",
    "typescript": "^5.6.3",
    "vite": "npm:rolldown-vite@^7.0.5",
    "vue": "^3.5.17"
  },
  "dependencies": {
    "vue-tsc": "^3.0.1"
  }
