{
  "name": "rolldown-vite-monorepo",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "start": "pnpm run build:package && pnpm run dev",
    "dev": "pnpm --filter demo-app dev",
    "build": "pnpm --filter package build && pnpm --filter app build",
    "build:package": "pnpm --filter shared-utils build",
    "build:app": "pnpm --filter app build"
  },
  "packageManager": "pnpm@10.12.1",
  "devDependencies": {
    "typescript": "^5.6.3"
  }
}
